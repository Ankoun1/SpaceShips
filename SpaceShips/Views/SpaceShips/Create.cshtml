<h1 class="text-center mt-2">Register</h1>
<hr class="bg-primary w-75">
<form class="mx-auto w-50" method="post">
    <div class="form-group">
        <div class="d-flex justify-content-start">
            <label for="type" class="h4 mb-2 text-dark">Type</label>
        </div>
        <input type="text" class="form-control" id="type" name="type" />
    </div>
    <div class="form-group">
        <div class="text-dark  d-flex justify-content-start">
            <label for="yearOfPurchase" class="h4 mb-2">Year Of Purchase</label>
        </div>
        <input type="text" class="form-control" id="yearOfPurchase" name="yearOfPurchase" />
    </div>
    <div class="form-group">
        <div class="text-dark  d-flex justify-content-start">
            <label for="yearOfTaxCalculation" class="h4 mb-2">Year Of TaxCalculation</label>
        </div>
        <input type="text" class="form-control" id="yearOfTaxCalculation" name="yearOfTaxCalculation" />
    </div>
    <div class="form-group">
        <div class="text-dark  d-flex justify-content-start">
            <label for="lightMilesTraveled" class="h4 mb-2">Light Miles Traveled</label>
        </div>
        <input type="text" class="form-control" id="lightMilesTraveled" name="lightMilesTraveled" />
    </div>
    <div class="d-flex justify-content-center">
        <button type="submit" class="btn btn-block btn-primary text-white mb-3" value="Register">Register</button>
    </div>
</form>

